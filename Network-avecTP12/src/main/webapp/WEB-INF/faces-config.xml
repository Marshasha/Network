<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <managed-bean>
  <managed-bean-name>managingNetworkParkBean</managed-bean-name>
  <managed-bean-class>ch.hevs.managedbeans.ManagingNetworkParkBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <display-name>welcomeNetwork.xhtml</display-name>
  <from-view-id>/welcomeNetwork.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>showAdminAccount</from-outcome>
   <to-view-id>/showAdminAccount.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>showAdminAccount.xhtml</display-name>
  <from-view-id>/showAdminAccount.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>manageParkIT</from-outcome>
   <to-view-id>/manageParkIT.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>justHaveALookOnPark</from-outcome>
   <to-view-id>/justHaveALookOnPark.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 

</faces-config>
