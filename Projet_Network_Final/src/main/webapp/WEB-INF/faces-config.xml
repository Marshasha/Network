<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <managed-bean>
  <managed-bean-name>distributionBean</managed-bean-name>
  <managed-bean-class>ch.hevs.managedbeans.DistributionBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <display-name>welcomeNetwork.xhtml</display-name>
  <from-view-id>/welcomeNetwork.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>showAdminAccount</from-outcome>
   <to-view-id>/showAdminAccount.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>showUserAccount</from-outcome>
   <to-view-id>/showUserAccount.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>showPopulateResult</from-outcome>
   <to-view-id>/showPopulateResult.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/showAdminAccount.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>manageParkIT</from-outcome>
   <to-view-id>/manageParkIT.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>manageUsers</from-outcome>
   <to-view-id>/manageUsers.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/manageParkIT.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>justHaveALookOnPark</from-outcome>
   <to-view-id>/justHaveALookOnPark.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>addDeviceForm</from-outcome>
   <to-view-id>/addDeviceForm.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/addNewUser.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>showInsertUserResult</from-outcome>
   <to-view-id>/showInsertUserResult.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/manageUsers.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>addNewUser</from-outcome>
   <to-view-id>/addNewUser.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>showUserDetailForManager</from-outcome>
   <to-view-id>/showUserDetailForManager.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/showUserAccount.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>showUserDetails</from-outcome>
   <to-view-id>/showUserDetails.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>showDeviceOfConnectedUser</from-outcome>
   <to-view-id>/showDeviceOfConnectedUser.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application/>
</faces-config>
